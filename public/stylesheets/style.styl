html, body, .map
  width: 100%
  height: 100%
body
  padding: 0
  margin: 0
  font: 12px/1.5 Verdana, sans-serif
.toolbox
  position:absolute
  bottom:25px
  width: 400px
  //height: 100px
  right: 25px
  z-index:9999
  padding: 1em
  background:#ffffff
.rounded
  border-radius: 0 0 8px 0
  -moz-border-radius: 0 0 8px 0
  -webkit-border-radius: 0 0 8px 0
  -khtml-border-radius: 0 0 8px 0
input, select
  margin-left: 10px
  border: 1px solid #000000
.long_text
  width: 300px
#header
  top: 5px
  left: 5px
  height: 40px
  width: 75%
  min-width: 760px
  position: fixed
#header, .test
  background: url("/images/cubes.png") repeat scroll 0 0 #FFFFFF
.test
  height: 100%
  width: 100%
.inner
  padding: 10px
#sidebar
  left: 5px
  position: fixed
  width: 300px
  min-height: 400px
  height: 75%
  top: 45px
  padding: 5px
  background: url("/images/cubes.png") repeat scroll 0 0 #FFFFFF
.results
  min-height: 400px
#header p
  padding: 0px 5px 0 5px
.right
  float: right
.left
  float: left
.clear:
  clear: all
.content
  padding:1.5em
  margin:0 0 1.5em
.dark
  background:#2a3333
.light
  background:#fff
.logo
  height:80px
.strong
  font-weight: bold
.holder
  padding: 5px
  margin: 0 0 0 5px
input, select
  background: none repeat scroll 0 0 #FFFFFF;
  border: medium none;
  border-radius: 5px 5px 5px 5px;
  outline: medium none;
  padding: 5px
  width: 300px
.hide
  display: none
.show
  display: block
.search
  height: 450px
.searches li
  list-style: none
  padding: 5px
.search, .searches
  padding: 0
.item
  border: 1px solid black;
#mapimage
  height: 300px
  width: 300px
#help
  padding-top: 10px
#help img
  width: 500px